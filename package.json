{
  "name": "fonsole",
  "productName": "fonsole",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "TideS <tidesmain@gmail.com>",
  "private": true,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node compile/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node compile/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node compile/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node compile/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node compile/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node compile/build.js",
    "build:web": "cross-env BUILD_TARGET=web node compile/build.js",
    "dev": "node compile/dev-runner.js",
    "dev:web": "cross-env IS_WEB=true node compile/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config compile/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config compile/webpack.renderer.config.js",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "postmerge": "node hooks/postmerge.js"
  },
  "build": {
    "productName": "fonsole",
    "appId": "com.fonsole.desktop",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "electron": "^1.6.11",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^7.0.0-alpha.18",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^2.0.0-alpha.18",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.1.2",
    "bluebird": "^3.5.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "del": "^2.2.1",
    "devtron": "^1.4.0",
    "electron-debug": "^1.3.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.5.0",
    "electron-rebuild": "^1.1.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "eslint-plugin-vue": "^3.8.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "extract-zip": "^1.6.5",
    "file-loader": "^0.11.1",
    "fonsole-networking": "git+https://github.com/ark120202/fonsole-networking.git",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "postcss": "^6.0.8",
    "postcss-loader": "^2.0.6",
    "proton": "git+https://github.com/a-jie/Proton.git",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vuex": "^2.3.1",
    "vuex-i18n": "^1.5.0",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "development": [
      "last 1 version"
    ]
  }
}
